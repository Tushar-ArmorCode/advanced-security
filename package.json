    "jest": "^29.4.2",
    "nock": "^13.0.5",
    "nodemon": "^2.0.21",
    "smee-client": "^1.2.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.0"
  },
  "engines": {
    "node": ">= 18.13.0"
