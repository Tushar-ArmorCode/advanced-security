    "jest": "^29.5.0",
    "nock": "^13.3.1",
    "nodemon": "^2.0.22",
    "smee-client": "^1.2.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">= 18.13.0"
